<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springbootdemo.Dao.PagelogMapper">
<!--    int insertlog(Pagelog pagelog);-->
    <insert id="insertlog">
        insert into pagelog values(#{id},#{name},#{loginNumber},#{consumption})
    </insert>
<!--    List<Pagelog> selectAll();-->
    <select id="selectAll" resultType="com.example.springbootdemo.entity.Pagelog">
        select * from pagelog
    </select>
<!--    int loginnumber();-->
    <select id="loginnumber" resultType="java.lang.Integer">
        SELECT sum(loginNumber) FROM pagelog
    </select>
<!--    int profitToday();-->
    <select id="profitToday" resultType="java.lang.Integer">
        SELECT sum(consumption) FROM pagelog
    </select>
</mapper>